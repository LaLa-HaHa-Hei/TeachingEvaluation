{
    "manifest_version": 3,
    "name": "TeachingEvaluation",
    "version": "1.0.0",
    "description": "自动评教",
    "permissions": [
        "scripting",
        "activeTab",
        "storage"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}